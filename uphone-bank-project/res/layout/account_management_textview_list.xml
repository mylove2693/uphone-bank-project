<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/accMana_rl" android:layout_width="fill_parent"
	android:layout_height="fill_parent">
	<ImageView android:id="@+id/accMana_ibStar"
		android:layout_marginLeft="21dip" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:layout_centerVertical="true">
	</ImageView>
	<TextView android:id="@+id/accMana_tvName"
		android:layout_toRightOf="@+id/accMana_ibStar"
		android:layout_centerVertical="true" android:layout_marginTop="10dip"
		android:layout_marginBottom="10dip" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:layout_marginLeft="5dip"
		android:singleLine="true" android:textSize="18sp" android:textColor="#ff000000"></TextView>
	<ImageView android:id="@+id/accMana_tvArrow"
		android:layout_marginRight="30dip" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:layout_alignParentRight="true"
		android:layout_centerVertical="true"></ImageView>
</RelativeLayout>
