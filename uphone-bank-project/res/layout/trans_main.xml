<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/bg"
    >
<include android:id="@+id/transabove" layout="@layout/main_bank_above" />
<include android:id="@+id/transbelow" layout="@layout/main_bank_bottom" />
<TableLayout
	android:layout_width="fill_parent"
	android:layout_height="wrap_content"
	android:layout_below="@id/transabove"
	android:layout_centerVertical="true"
	android:layout_centerHorizontal="true"
	>
<TableRow
	android:layout_marginLeft="60dip"
	>
<ImageButton
android:id="@+id/btn_trans_main_ph"
android:layout_width="80dip"
android:layout_height="80dip"
android:src="@drawable/transph"
android:scaleType="fitXY"
android:background="#00ffffff"
>
</ImageButton>
<ImageButton
android:id="@+id/btn_trans_main_acc"
android:layout_width="80dip"
android:layout_height="80dip"
android:layout_marginLeft="40dip"
android:src="@drawable/transacc"
android:scaleType="fitXY"
android:background="#00ffffff"
>
</ImageButton>
</TableRow>
<TableRow
android:layout_marginLeft="60dip"
>
<TextView
android:id="@+id/actManagerText"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:text="机机转账"
android:textStyle="bold"
android:gravity="center"
android:textColor="#ffffff"
>
</TextView>
<TextView
android:id="@+id/actActivateText"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:text="机卡转账"
android:textStyle="bold"
android:gravity="center"
android:layout_marginLeft="40dip"
android:textColor="#ffffff"
>
</TextView>
</TableRow>
</TableLayout>
</RelativeLayout>