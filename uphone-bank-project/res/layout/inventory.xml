<?xml version="1.0" encoding="UTF-8"?>
<RelativeLayout android:layout_width="fill_parent"
              android:id="@+id/inventory" 
              android:layout="@layout/inventory"
	          android:layout_height="fill_parent" 
	          xmlns:android="http://schemas.android.com/apk/res/android"
	          android:background="@drawable/accountque">
	<include android:id="@+id/accManaAbove" layout="@layout/main_bank_above" />
	<include android:id="@+id/accManaBottom" layout="@layout/main_bank_bottom" />
	<LinearLayout android:layout_width="fill_parent"
		android:layout_below="@id/accManaAbove" android:layout_height="fill_parent"
		android:orientation="vertical">
		<TextView android:id="@+id/inventory1"
	          android:layout_height="wrap_content" 
	          android:layout_width="wrap_content"
	          android:textSize="20dp"
	          android:text="请输入你要查询的时间区间"
	          android:layout_marginLeft="38dip"
	          android:textColor="#000000">
	        </TextView>
	        <TextView 
	          android:layout_height="1dip" 
	          android:layout_width="fill_parent"
	          android:background="#999999ff"
	          android:layout_marginTop="3dip">
	        </TextView>
			<TextView android:id="@+id/inventory_c"
	          android:layout_height="wrap_content" 
	          android:layout_width="wrap_content"
	          android:textSize="20dp"
	          android:text="开始时间"
	          android:textColor="#000000"
	          android:gravity="left"
	          android:layout_marginTop="10dip"
	          android:layout_marginLeft="30dip"
	          android:layout_marginRight="10dip"/>
	          <TextView 
	          android:layout_height="1dip" 
	          android:layout_width="fill_parent"
	          android:background="#999999ff"
	          android:layout_marginTop="3dip">
	        </TextView>
	        <Button  android:id="@+id/timechange_from"
	          android:layout_height="wrap_content" 
	          android:layout_width="fill_parent"
	          android:gravity="center"
	          android:layout_marginTop="3dip"
	          android:layout_marginLeft="90dip"
	          android:layout_marginRight="30dip"
	          android:text="起始时间">
	        </Button>
	        <TextView 
	          android:layout_height="1dip" 
	          android:layout_width="fill_parent"
	          android:background="#999999ff"
	          android:layout_marginTop="1dip">
	        </TextView>
			<TextView android:id="@+id/inventory_c"
	          android:layout_height="wrap_content" 
	          android:layout_width="wrap_content"
	          android:textSize="20dp"
	          android:text="终止时间"
	          android:textColor="#000000"
	          android:gravity="left"
	          android:layout_marginTop="10dip"
	          android:layout_marginLeft="30dip">
	        </TextView>
	        <TextView 
	          android:layout_height="1dip" 
	          android:layout_width="fill_parent"
	          android:background="#999999ff"
	          android:layout_marginTop="10dip">
	        </TextView>
	        <Button  android:id="@+id/timechange_to"
	          android:layout_height="wrap_content" 
	          android:layout_width="fill_parent"
	          android:gravity="center"
	          android:layout_marginTop="3dip"
	          android:layout_marginLeft="90dip"
	          android:layout_marginRight="30dip"
	          android:text="结束时间">
	        </Button>
	        <TextView 
	          android:layout_height="1dip" 
	          android:layout_width="fill_parent"
	          android:background="#999999ff"
	          android:layout_marginTop="1dip">
	        </TextView>
			<Button  android:id="@+id/Query_Run"
	          android:layout_height="wrap_content" 
	          android:layout_width="wrap_content"
	          android:text="查 询 ->"
	          android:layout_marginTop="10dip"
	          android:layout_marginLeft="130dip">
	        </Button>
	</LinearLayout>
</RelativeLayout>