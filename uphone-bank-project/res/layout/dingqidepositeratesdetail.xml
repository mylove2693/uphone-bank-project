<?xml version="1.0" encoding="utf-8"?>
<!-- 定期存款利率Activity 布局 -->
<RelativeLayout
android:id="@+id/selfCompanydepositeRatesLayout"
android:layout_width="fill_parent"
android:layout_height="fill_parent"
android:background="@drawable/bg"
xmlns:android="http://schemas.android.com/apk/res/android">
<!-- 定期存款利率 文本布局  距离上面的上外边距20dip-->
<LinearLayout
android:id="@+id/layout1"
android:layout_width="fill_parent"
android:layout_height="wrap_content"
>
<!-- 定期存款利率 文本 -->
<TextView
android:id="@+id/selfPayText"
android:layout_width="fill_parent"
android:layout_height="wrap_content"
android:text="定期存款利率"
android:textSize="20sp"
android:textStyle="bold"
android:textColor="#ffffff"
android:gravity="center"/>
</LinearLayout>

<!-- 整存整取 外布局 有背景 -->
<LinearLayout
android:id="@+id/out1Layout"
android:layout_width="fill_parent"
android:layout_height="wrap_content"
android:orientation="vertical"
android:background="#ff33ff"
android:layout_below="@id/layout1"
>
<!-- 整存整取 文本布局  -->
<LinearLayout
android:id="@+id/selfCompanydepositeRatesTiSiTextLayout"
android:layout_width="fill_parent"
android:layout_height="wrap_content"
>
<!-- 整存整取文本  -->
<TextView
android:id="@+id/selfPayText"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:text="整存整取"
android:textSize="18sp"
android:textStyle="bold"
android:textColor="#ffffff"
android:layout_marginLeft="10dip"
>
</TextView>
</LinearLayout>
<!-- 分隔线 距离上面 外上边距 10dip-->
<TextView
	android:layout_width="fill_parent"
	android:layout_height="2dip"
	android:background="#ffffffff"/>
<!--三个月文本布局  -->
<LinearLayout 
	android:id="@+id/activatedDepositeRatesLayout"
	xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal">
<!-- 三个月 文本 距离左边距30dip -->
<TextView 
  android:id="@+id/activatedDepositeRatesTiSi" 
  android:layout_width="wrap_content" 
  android:layout_height="wrap_content" 
  android:text="三个月"
  android:textColor="#ffffff"
  android:textSize="16sp"
 /> 
 <!-- 三个月利率值  距离左边三个月文本的左边距  130dip-->
  <TextView 
  android:id="@+id/activatedDepositeRatesText" 
  android:layout_width="wrap_content" 
  android:layout_height="wrap_content" 
  android:text="2.25%"
  android:textColor="#ffffff"
  android:textSize="16sp"
 /> 
</LinearLayout>
<!-- 分隔线 -->
<TextView
	android:layout_width="fill_parent"
	android:layout_height="2dip"
	android:background="#ffffffff"
	/>
<!-- 六个月文本布局 -->
<LinearLayout 
	android:id="@+id/DingQiDepositeRatesLayout" 
	xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal">
<!-- 六个月文本 距离左边距30dip -->
<TextView 
  android:id="@+id/OtherItemText" 
  android:layout_width="wrap_content" 
  android:layout_height="wrap_content" 
  android:textColor="#ffffff"
  android:textSize="16sp"
  android:text="六个月"
 /> 
 <!-- 六个月利率值  距离左边六个月文本左边距是130dip -->
 <TextView 
  android:id="@+id/activatedDepositeRatesText" 
  android:layout_width="wrap_content" 
  android:layout_height="wrap_content" 
  android:text="2.5%"
  android:textColor="#ffffff"
  android:textSize="16sp"
 /> 
</LinearLayout>
<!-- 分隔线 -->
<TextView
	android:layout_width="fill_parent"
	android:layout_height="2dip"
	android:background="#ffffffff"
	/>
<!-- 一年文本布局 距离上面上边距 10dip-->
<LinearLayout 
	android:id="@+id/oneYearLayout" 
	xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal">
<!-- 一年 文本 距离左边距30dip-->    
<TextView 
  android:id="@+id/OtherItemText" 
  android:layout_width="wrap_content" 
  android:layout_height="wrap_content" 
  android:textColor="#ffffff"
  android:textSize="16sp"
  android:text="一年"
 /> 
 <!-- 一年利率值 距离一年文本左边距144dip -->
 <TextView 
  android:id="@+id/activatedDepositeRatesText" 
  android:layout_width="wrap_content" 
  android:layout_height="wrap_content" 
  android:text="2.75%"
  android:textColor="#ffffff"
  android:textSize="16sp"
 /> 
</LinearLayout>
</LinearLayout>
<!--零存整取 外布局 有背景-->  
<LinearLayout
android:id="@+id/out2Layout"
android:layout_width="fill_parent"
android:layout_height="wrap_content"
android:orientation="vertical"
