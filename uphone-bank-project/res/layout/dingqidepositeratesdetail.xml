  android:layout_width="wrap_content" 
  android:layout_height="wrap_content" 
  android:textColor="#ffffff"
  android:textSize="16sp"
  android:text="三年"
 /> 
 <!-- 三年利率值 距离左边三年文本距离142dip -->
 <TextView 
  android:id="@+id/activatedDepositeRatesText" 
  android:layout_width="wrap_content" 
  android:layout_height="wrap_content" 
  android:text="4.15%"
  android:textColor="#ffffff"
